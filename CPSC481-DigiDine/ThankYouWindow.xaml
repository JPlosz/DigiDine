<Window x:Class="CPSC481_DigiDine.ThankYouWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CPSC481_DigiDine"
        mc:Ignorable="d"
        Title="ThankYouWindow" Height="300" Width="600" Background="{StaticResource backgroundColor}">
    <Grid>
        <TextBlock HorizontalAlignment="Center" TextWrapping="Wrap" Text="Thank You For Your Order!" VerticalAlignment="Center" FontSize="72" TextAlignment="Center" Foreground="{StaticResource textColor}" FontWeight="Bold"/>

    </Grid>
</Window>
